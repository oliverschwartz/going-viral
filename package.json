{
  "name": "going-viral",
  "repository": "going-viral",
  "version": "0.1.0",
  "title": "package",
  "description": "An arcade-style survival game. Beat the viruses and get to the finish!",
  "keywords": [
    "Princeton",
    "graphics",
    "three.js",
    "cannon.js",
    "teamwork",
    "fun",
    "COVID-19"
  ],
  "scripts": {
    "start": "webpack-dev-server --mode development --output-public-path '/'",
    "prebuild": "rimraf ./build && mkdir ./build",
    "build": "npm run prebuild && webpack --mode production --output-public-path '/'",
    "format": "prettier --write '*.js' 'src/**/*.js'",
    "deploy": "npm run prebuild && webpack --mode production && gh-pages -d build"
  },
  "dependencies": {
    "bootstrap": "^4.4.1",
    "cannon": "^0.6.2",
    "css-loader": "^3.5.3",
    "dat.gui": "^0.7.7",
    "jquery": "^3.5.1",
    "style-loader": "^1.2.1",
    "three": "^0.137.0",
    "three-obj-loader": "^1.1.3",
    "typeface-sigmar-one": "0.0.72",
    "webpack-cli": "^3.3.11"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "babel-loader": "^8.1.0",
    "file-loader": "^6.0.0",
    "gh-pages": "^2.2.0",
    "html-webpack-plugin": "^4.2.1",
    "raw-loader": "^4.0.1",
    "rimraf": "^3.0.2",
    "webpack": "^4.43.0",
    "webpack-dev-server": "^3.10.3",
    "webpack-obj-loader": "^1.0.4"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
